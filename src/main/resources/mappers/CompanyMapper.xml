<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jobfinder.persistence.CompanyMapper">

    <select id="list" resultType="Company_info">
        select company_id, company_name, company_industry, company_logo, company_type from company_detail
    </select>

    <select id="detail" parameterType="Company_info" resultType="Company_info">
        select * from company_detail where company_id = #{company_id}
    </select>
</mapper>